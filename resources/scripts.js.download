$(document).ready(function () {
    // Trigger custom event "hide" and "show"; each child input also trigger this event
    (function ($) {
        $.each(['show', 'hide'], function (i, ev) {
            var el = $.fn[ev];
            $.fn[ev] = function () {
                this.trigger(ev);
                this.children().find("input").each(function () {
                    $(this).trigger(ev);
                });
                return el.apply(this, arguments);
            };
        });
    })(jQuery);
    var displayMode = $("#hdnDisplayMode");

    if (displayMode.val() == "Edit") {
        MultilineTextBoxMaxLengthLimit();
        MultilineTextBoxDivWidth();
        InitializeSortingInformation();
        InitializeApplicantInformationEvents();
        InitializeDriverLicenseIDCardInformationEvents();
        InitializePriorClaimInformationEvents();
        InitializeContactInformationEvents();
        InitializeDigitalPaymentOptionInformationEvents();
        InitializeLastEmployerInformationEvents();
        InitializeEnterLastEmployerDetails();
        InitializeEmploymentInformationEvents();
        InitializeAvailabilityInformationEvents();
        InitializeCitizenshipInformationEvents();
        InitalizeOtherInformationEvents();
        InitializeDisasterUnemploymentInformationEvents();
        InitializeSchoolEmployeeInformationEvents();
    }
    else {
        InitializeApplicantInformationReadOnly();
        InitializeDMVInformationReadOnly();
        InitializePriorClaimInformationReadOnly();
        InitializeContactInformationReadOnly();
        InitializeDigitalPaymentOptionInformationReadOnly();
        InitializeCitizenshipInformationReadOnly();
        InitializeLastEmployerInformationReadOnly();
        InitializeEmploymentInformationReadOnly();
        InitializeSchoolEmployeeInformationReadOnly();
        InitializeAvailabilityInformationReadOnly();
        InitalizeOtherInformationReadOnly();
        InitializeDisasterUnemploymentInformationReadOnly();
        SurveyMultilineTextBoxMaxLengthLimit();
    }
});

