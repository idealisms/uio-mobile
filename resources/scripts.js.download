        var rbs = gv.getElementsByTagName("input");

        // Below unchecks (if checked) the optional outer radio button (example. "Employer not listed above")
        var rbOuter;
        if (rbOuterID != undefined) {
            rbOuter = document.getElementById(rbOuterID);
            rbOuter.checked = false;
        }

        var row = rb.parentNode.parentNode;
        for (var i = 0; i < rbs.length; i++) {
            if (rbs[i].type == "radio") {
                if (rbs[i].checked && rbs[i] != rb) {
                    rbs[i].checked = false;
                    break;
                }
            }
        }
    }
