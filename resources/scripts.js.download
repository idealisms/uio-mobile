// BEGIN - SCR_NC_50 – Employment Information
function InitializeEmploymentInformationEvents() {
    DisplayAddEmployerInformation();
    DisableAndAddPlaceholderText();

    $("#gvBasePeriodEmployers input[type='radio']").each(function (index) {
        $(this).attr("id", $(this).attr("id") + index);
    });
    $("#gvBasePeriodEmployers label").each(function (index) {
        $(this).attr("for", $(this).attr("for") + index);
    });
    $("#prtWorkedForAnyOtherEmployerNotListed input[type='radio']").each(function (index) {
        $(this).attr("id", $(this).attr("id") + "prtWorkedForAnyOtherEmployerNotListed" + index);
    });
    $("#prtWorkedForAnyOtherEmployerNotListed label").each(function (index) {
        $(this).attr("for", $(this).attr("for") + "prtWorkedForAnyOtherEmployerNotListed" + index);
    });

    $("#prtWorkedForAnyEmployer input[type=radio]").change(function () {
        DisplayAddEmployerInformation();
    });

    $("#gvBasePeriodEmployers tr").each(function (index) {
        if (index > 0) {
            var rbList = $(this).find("input[type=radio]");
            var isAdditionalInformationProvided = $(this).find("#hdnIsAdditionalInformationProvided");
            var updateLink = $(this).find("#lblUpdate");

            var additionalInformationStatus = $(this).find("#lblIsAdditionalInformationProvided");
            additionalInformationStatus.text(EmployerAdditionalInformationStatus(rbList, ConvertToBoolean(isAdditionalInformationProvided.val())));

            if (rbList[0].checked == true) {
                updateLink.show();
            }
            else {
                updateLink.hide();
            }
        }
    });
}

function DisplayAddEmployerInformation() {
    var rbOpt = $("#prtWorkedForAnyEmployer input[type=radio]");
    var prtWorkedForAnyOtherEmployerNotListed = $("#prtWorkedForAnyOtherEmployerNotListed input[type=radio]");
    if (rbOpt.length != 0 && rbOpt[0].checked == true) {
        $(".addEmployerInformationGroup").show();
    }
    else if (rbOpt.length != 0) {
        if (rbOpt[1].checked == true) {
            prtWorkedForAnyOtherEmployerNotListed[1].checked = true;
        }
        $(".addEmployerInformationGroup").hide();
    }
}

function WorkedForEmployerChanged(rbSpanList, rowIndex, isAdditionalInformationProvided) {
    var gvRowIndex = parseInt(rowIndex) + 1;
    var rbList = rbSpanList.getElementsByTagName("input");
    var status = EmployerAdditionalInformationStatus(rbList, ConvertToBoolean(isAdditionalInformationProvided));
    $($("#gvBasePeriodEmployers tr").get(gvRowIndex)).find("#lblIsAdditionalInformationProvided").text(status);

    var updateLink = $($("#gvBasePeriodEmployers tr").get(gvRowIndex)).find("#lblUpdate");
    if (rbList[0].checked == true) {
        updateLink.show();
    }
    else {
        updateLink.hide();
    }
}

function EmployerAdditionalInformationStatus(rbList, isAdditionalInformationProvided) {
    var returnVal = "";

    var currSelection;
    if (rbList[0].checked == true) {
        currSelection = true;
    }
    else if (rbList[1].checked == true) {
        currSelection = false;
    }

    if (currSelection != undefined) {
        if (currSelection == true) {
            if (isAdditionalInformationProvided == true) {
                returnVal = EmploymentInformationLabels.StatusComplete;
            }
            else {
                returnVal = EmploymentInformationLabels.StatusIncomplete;
            }
        }
        else {
            returnVal = EmploymentInformationLabels.StatusNotApplicable;
        }
    }
    else {
        returnVal = EmploymentInformationLabels.StatusIncomplete;
    }

    return returnVal;
}

function ConvertToBoolean(value) {
    if (value == undefined) {
        return;
    }
    else {
        return value.toString().toLowerCase() == "true" ? true : false;
    }
}

function DisableAndAddPlaceholderText() {
    var prtBusinessType = $("#prtBusinessType textarea");
    var prtWorkType = $("#prtWorkType textarea");
    if (prtBusinessType.length != 0) {
        if (prtBusinessType.val() == "") {
            prtBusinessType.attr('placeholder', EmploymentInformationLabels.PlaceholderAddBusinessType);
        }
        prtBusinessType.attr("disabled", "disabled");
    }
    if (prtWorkType.length != 0) {
        if (prtWorkType.val() == "") {
            prtWorkType.attr('placeholder', EmploymentInformationLabels.PlaceholderAddWorkType);
        }
        prtWorkType.attr("disabled", "disabled");
    }
}

