function InitializeLastEmployerInformationReadOnly() {
    var lastDateWorked = $("#prtLastDateWorked").text();

    // If Claimant entered a date and it is in a valid format (MM/DD/YYYY)
    var firstDay = $("#hdnEffectiveDate").val();  // getFirstDayOfTheWeek();
    var currentDay = $("#hdnCurrentDate").val();  // getFirstDayOfTheWeek();
    var lastDay = $("#hdnWeekEndDate").val(); //getLastDayOfTheWeek();
    var dayIndex = new Date(lastDateWorked).getDay();
    var enable = false;
    enable = (compareDates("GreaterThanEqual", lastDateWorked, firstDay) && compareDates("LessThanEqual", lastDateWorked, lastDay) && compareDates("LessThanEqual", lastDateWorked, currentDay));
    if (enable) {
        // Display the wages text boxes; disable those that are less than the current day.
        $(".trWagesInfo").show();
        $("[id*='prtDailyGrossWages'] span").each(function (index) {
            if (index <= dayIndex) {
                $(this).show()
            }
            else {
                $(this).hide()
            }
        });
    }
    else {
        // hide the wages text boxes
        $(".trWagesInfo").hide();
    }
}

