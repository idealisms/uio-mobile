function EmployerAdditionalInformationStatusReadOnly(rbReadOnlyValList, isAdditionalInformationProvided) {
    var returnVal = "";

    var currSelection;
    if (rbReadOnlyValList.text().indexOf(ReadOnlyLabels.RadioButtonYes) != -1) {
        currSelection = true;
    }
    else if (rbReadOnlyValList.text().indexOf(ReadOnlyLabels.RadioButtonNo) != -1) {
        currSelection = false;
    }

    if (currSelection != undefined) {
        if (currSelection == true) {
            if (isAdditionalInformationProvided == true) {
                returnVal = ReadOnlyLabels.StatusComplete;
            }
            else {
                returnVal = ReadOnlyLabels.StatusIncomplete;
            }
        }
        else {
            returnVal = ReadOnlyLabels.StatusNotApplicable;
        }
    }
    else {
        returnVal = ReadOnlyLabels.StatusIncomplete;
    }

    return returnVal;
}

