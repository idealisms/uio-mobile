//function to display Disaster WorkerType Information

function DisplayDisasterWorkerTypeInformation() {
    var ddOpt = $("#prtDisasterWorkerType select");
    //debugger;
    if (ddOpt.length > 0 && !ddOpt.is(":disabled")) {
        if ((ddOpt.val() == DisasterWorkerType.UnemployedWorker)) {
            $("#txtDisasterHrsPerWeek input").removeAttr("disabled");
            $("#txtDisasterExplanation textarea, #txtDisasterStreet input, #txtDisasterCity input, #txtDisasterZip input, #optDisasterState select").attr("disabled", "disabled");
        }

        if ((ddOpt.val() == DisasterWorkerType.UnemployedSelfemployedIndividual)) {
            $("#txtDisasterExplanation textarea, #txtDisasterStreet input, #txtDisasterCity input, #txtDisasterZip input, #optDisasterState select, #txtDisasterHrsPerWeek input").removeAttr("disabled");
        }
        if ((ddOpt.val() == DisasterWorkerType.UnemployedSelfemployedIndividualWithProspectiveSelfEmployment)) {
            $("#txtDisasterExplanation textarea, #txtDisasterStreet input, #txtDisasterCity input, #txtDisasterZip input, #optDisasterState select").removeAttr("disabled");
            $("#txtDisasterHrsPerWeek input").attr("disabled", "disabled");
        }
        if ((ddOpt.val() == DisasterWorkerType.UnemployedWorkerWithProspectiveEmployment) || (ddOpt.val() == DisasterWorkerType.HeadofHouseholdAsaResultoftheDisaster) || ddOpt.val() == 0) {
            $("#txtDisasterExplanation textarea, #txtDisasterStreet input, #txtDisasterCity input, #txtDisasterZip input, #optDisasterState select, #txtDisasterHrsPerWeek input").attr("disabled", "disabled");
        }
    }
}

