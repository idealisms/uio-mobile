function DisplayUSCISControls(lstINSDoc) {
    /********************************************************************************************************************************************
    'USCISDocumentType' is the collection variable defined in CitizenshipInformationTemplate.ascx which internally binds
    EDD.CCR.Utility.ReferenceTableConstant values using server tags.
    *********************************************************************************************************************************************/

    if (USCISDocumentType != undefined) {
        if ($(lstINSDoc).parent().attr("class") != "aspNetDisabled") {
            if ($(lstINSDoc).val() == USCISDocumentType.UnexpPassport) {
                $("#optIsLegalToWorkInUs input,#txtDocumentExpirationDate input,#txtPassportNumber input, #txtArrivalOrDepartureNumber input,#prtPassportCountry select").removeAttr("disabled");
            }
            if ($(lstINSDoc).val() == USCISDocumentType.RentryPermitI327 || $(lstINSDoc).val() == USCISDocumentType.RefugeeTravelDocumentI571 || $(lstINSDoc).val() == USCISDocumentType.EmpAuthCardI766) {
                $("#optIsLegalToWorkInUs input,#txtDocumentExpirationDate input,#txtAlienRegNumber input").removeAttr("disabled");
            }
            if ($(lstINSDoc).val() == USCISDocumentType.PermResidentCardI551 || $(lstINSDoc).val() == USCISDocumentType.EmpAuthCardI766) {
                $("#optIsLegalToWorkInUs input,#txtDocumentExpirationDate input,#txtAlienRegNumber input,#txtPermanentResidentCardNbr input").removeAttr("disabled");
            }
            if ($(lstINSDoc).val() == USCISDocumentType.ArrDepRecordI94) {
                $("#optIsLegalToWorkInUs input,#txtDocumentExpirationDate input,#txtArrivalOrDepartureNumber input").removeAttr("disabled");
            }
            if ($(lstINSDoc).val() == USCISDocumentType.ArrDepRecordI94UnexpPassport) {
                $("#optIsLegalToWorkInUs input,#txtDocumentExpirationDate input,#txtArrivalOrDepartureNumber input,#txtPassportNumber input,#txtVisaNumber input,#prtPassportCountry select").removeAttr("disabled");
            }
            if ($(lstINSDoc).val() == USCISDocumentType.OtherDocument) {
                $("#optIsLegalToWorkInUs input,#txtDocumentExpirationDate input,#txtArrivalOrDepartureNumber input,#txtAlienRegNumber input,#txtOtherDocumentDescription input, #txtPassportNumber input, #prtPassportCountry select").removeAttr("disabled");
            }
        }
    }
}

