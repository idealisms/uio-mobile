// BEGIN - SCR_NC_40 – Last Employer Information
function InitializeLastEmployerInformationEvents() {
    WagesInformationDisplay(); // Call to ensure controls are displayed on pageload if claimant already provided
    TotalWagesCalculation(); // Call to ensure total wages are calculated on pageload
    OtherIncomeDisplay(); // Call to ensure controls are enabled/disabled based on previous claimant answers
    $(".estimatedCheckBoxTopAlign").each(function (index) {
        $(this).find("input[type='checkbox']").attr("id", "chkValue" + index);
        $(this).find("label").attr("for", "chkValue" + index);
    });

    // When LastDateWorked changes, display the wages textboxes (disabled/enabled as required)
    $("#prtLastDateWorked input[type='text']").bind('input propertychange paste change', function () {
        WagesInformationDisplay();
    });

    // When any wages text box is updated, total wages should recalculate
    $("[id*='prtDailyGrossWages']").each(function () {
        $(this).bind('input propertychange paste change', function () {
            TotalWagesCalculation();
        });
    });

    $("#divOtherIncomeInformation input[type='checkbox']").each(function () {
        $(this).change(function () {
            OtherIncomeDisplay();
        });
    });
}

// Other Types of Income Functions
function OtherIncomeDisplay() {
    $("#divOtherIncomeInformation input[type='checkbox']").each(function (index) {
        var checkBoxIndex = index;
        if ($(this).is(':checked') == true) {
            $("#divOtherIncomeInformation input[type='text']").each(function (index) {
                var trueIndex = parseInt(parseFloat(index / 3));
                if (checkBoxIndex == undefined || checkBoxIndex == trueIndex) {
                    if ($(this).parent().attr("class") != "aspNetDisabled") {
                        $(this).removeAttr("disabled");
                    }
                }
            });
            $("#divOtherIncomeInformation input[type='image']").each(function (index) {
                var trueIndex = parseInt(parseFloat(index / 2));
                if (checkBoxIndex == undefined || checkBoxIndex == trueIndex) {
                    if ($(this).parent().attr("class") != "aspNetDisabled") {
                        $(this).removeAttr("disabled");
                        $(this).css("opacity", "1.0");
                    }
                }
            });
            if (checkBoxIndex == ($("#divOtherIncomeInformation input[type='checkbox']").length - 1)) {
                if ($("#prtOtherPayComment textarea").parent().attr("class") != "aspNetDisabled") {
                    $("#prtOtherPayComment textarea").removeAttr("disabled");
                }
            }
        }
        else {
            $("#divOtherIncomeInformation input[type='text']").each(function (index) {
                var trueIndex = parseInt(parseFloat(index / 3));
                if (checkBoxIndex == undefined || checkBoxIndex == trueIndex) {
                    $(this).attr("disabled", "true");
                    $(this).val("");
                }
            });
            $("#divOtherIncomeInformation input[type='image']").each(function (index) {
                var trueIndex = parseInt(parseFloat(index / 2));
                if (checkBoxIndex == undefined || checkBoxIndex == trueIndex) {
                    $(this).attr("disabled", "true");
                    $(this).css("opacity", "0.5");
                }
            });
            if (checkBoxIndex == ($("#divOtherIncomeInformation input[type='checkbox']").length - 1)) {
                $("#prtOtherPayComment textarea").attr("disabled", "true");
                $("#prtOtherPayComment textarea").val("");
            }
        }
    });
}

// Wages Information Functions
function WagesInformationDisplay() {
    var lastDateWorked = $("#prtLastDateWorked input[type='text']").val();
    $("#prtTotalDailyGrossWages input[type='text']").attr("disabled", "true");

    if (lastDateWorked != "" && isValidDateFormat(lastDateWorked) == true) {
        // If Claimant entered a date and it is in a valid format (MM/DD/YYYY)
        var firstDay = $("#hdnEffectiveDate").val();  // getFirstDayOfTheWeek();
        var currentDay = $("#hdnCurrentDate").val();  // getFirstDayOfTheWeek();
        var lastDay = $("#hdnWeekEndDate").val(); //getLastDayOfTheWeek();
        var dayIndex = new Date(lastDateWorked).getDay();
        var enable = false;
        if (isValidCalendarDate(lastDateWorked) == true) {
            enable = (compareDates("GreaterThanEqual", lastDateWorked, firstDay) && compareDates("LessThanEqual", lastDateWorked, lastDay) && compareDates("LessThanEqual", lastDateWorked, currentDay));
        }
        if (enable) {
            // Display the wages text boxes; disable those that are less than the current day.
            $(".trWagesInfo").show();
            $("[id*='prtDailyGrossWages'] input[type='text']").each(function (index) {
                if (index <= dayIndex) {
                    if ($(this).parent().attr("class") != "aspNetDisabled") {
                        $(this).removeAttr("disabled");
                    }
                }
                else {
                    $(this).attr("disabled", "true");
                    $(this).val(""); // Remove Wages
                }
            });
        }
        else {
            // hide the wages text boxes
            $(".trWagesInfo").hide();
            $("[id*='prtDailyGrossWages'] input[type='text']").each(function () {
                $(this).val(""); // Remove Wages
            });
        }
    }
    else {
        // hide the wages text boxes
        $(".trWagesInfo").hide();
        $("[id*='prtDailyGrossWages'] input[type='text']").each(function () {
            $(this).val(""); // Remove Wages
        });
    }
    TotalWagesCalculation();
}

function TotalWagesCalculation() {
    var totalWages = 0;
    $("[id*='prtDailyGrossWages'] input[type='text']").each(function () {
        if (isValidNumber($(this).val()) == true) {
            totalWages += parseFloat($(this).val()) || 0;
        }
    });
    if (totalWages == 0) {
        totalWages = "0.00";
    }
    else {
        totalWages = totalWages.toFixed(2);
    }
    $("#prtTotalDailyGrossWages input[type='text']").val(totalWages);
}

// Helper Functions
function isValidDateFormat(value) {
    var validDateFormat = new RegExp("^\\d{2}\/\\d{2}\/\\d{4}$");
    var returnValue = false;
    if (value != undefined) {
        if (validDateFormat.test(value)) {
            returnValue = true;
        }
    }
    return returnValue;
}

function isValidNumber(value) {
    var validNumFormat = new RegExp("^\\d{0,6}(\\.\\d{1,2}|\\.)?$");
    var returnValue = false;
    if (value != undefined) {
        if (validNumFormat.test(value)) {
            returnValue = true;
        }
    }
    return returnValue;
}

function compareDates(operand, val1, val2) {
    var date1 = new Date(val1);
    var date2 = new Date(val2);
    var returnVal = false;

    if (operand == "GreaterThanEqual") {
        if (date1 >= date2) {
            returnVal = true;
        }
    }
    else if (operand == "LessThanEqual") {
        if (date1 <= date2) {
            returnVal = true;
        }
    }
    return returnVal;
}
