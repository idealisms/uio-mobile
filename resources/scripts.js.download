function FormatInput(value) {
    var returnVal = value;
    if (returnVal != undefined) {
        if (returnVal.length >= 3) {
            if (returnVal.length < 7) {
                returnVal = returnVal.substr(0, 3) + returnVal.substr(4);
            }
            else if (returnVal.length >= 7 && returnVal.length <= 11) {
                returnVal = returnVal.substr(0, 3) + returnVal.substr(4, 2) + returnVal.substr(7);
            }
        }
    }
    return returnVal;
}
